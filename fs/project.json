{
  "name": "fs",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo run --release -- --output disk.img --dir root --size 512",
        "cwd": "fs"
      },
      "outputs": [
        "{workspaceRoot}/target/release/fs"
      ]
    },
    "nx-release-publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'Skipping Cargo publish for kernel'",
        "parallel": false
      }
    }
  }
}
